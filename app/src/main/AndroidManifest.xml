<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.tiktok">

    <application
        android:allowBackup="true"
        android:name=".lifecycle.MyApplication"
        android:icon="@mipmap/tiktok_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lifecycle.LifeCycleActivity" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.RelativeLayoutActivity" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MessagePage" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".chat.chatActivity" android:theme="@style/AppTheme.NoActionBar" />
    </application>


</manifest>